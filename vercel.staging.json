{
  "name": "dagger-staging",
  "functions": {
    "api/chat.js": {
      "maxDuration": 300
    }
  },
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "NODE_ENV": "staging",
    "VERCEL_ENV": "preview"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Environment",
          "value": "staging"
        }
      ]
    }
  ]
}